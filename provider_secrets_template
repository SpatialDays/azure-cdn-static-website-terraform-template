# Do not edit this file!
# It is used to create a new file using envsubst, filling
# it with the values from the environment.
# Finally, newly created file is used to init the terraform.
subscription_id = "${TF_VAR_SUBSCRIPTION_ID}"
tenant_id       = "${TF_VAR_TENANT_ID}"
client_id       = "${TF_VAR_CLIENT_ID}"
client_secret   = "${TF_VAR_CLIENT_SECRET}"
resource_group_name  = "${TF_VAR_STATE_RESOURCE_GROUP_NAME}"
storage_account_name = "${TF_VAR_STATE_STORAGE_ACCOUNT_NAME}"
container_name       = "${TF_VAR_STATE_CONTAINER_NAME}"
key                  = "${TF_VAR_ENVIRONMENT}.tfstate"